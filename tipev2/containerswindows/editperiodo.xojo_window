#tag WindowCode
	#tag Method, Flags = &h21
		Private Sub actualizartramo()
		  mtramo.aula=Comboaula.Text
		  dim diasemana as integer
		  Select case popdiasemana.ListIndex
		  case 0 //lunes
		    diasemana=2
		  case 1 //martes
		    diasemana=3
		  case 2 //miercoles
		    diasemana = 4
		  case 3 //jueves
		    diasemana= 5
		  case 4 //viernes
		    diasemana = 6
		  case 5 //sabado
		    diasemana = 7
		  case 6 //domingo
		    diasemana = 8
		  End Select
		  mtramo.dia_semana=diasemana
		  mtramo.h_fin=Combohorafin.Text
		  mtramo.h_inicio=Combohorainicio.Text
		  if Popupgrupomateria.ListIndex<>-1 then
		    mtramo.id_grupo_rel=Popupgrupomateria.RowTag(Popupgrupomateria.ListIndex)
		  end if
		  mtramo.periodo=val(Comboperiodo.Text)
		  
		  if not mtramo.guardar then
		    MsgBox ("No se ha podido guardar el tramo horario, hay un problema con la base de datos")
		  else
		    MsgBox ("el tramo horario se ha guardado correctamente")
		  end if
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h21
		Private mtramo As tramo_horario
	#tag EndProperty

	#tag ComputedProperty, Flags = &h0
		#tag Getter
			Get
			  return mtramo
			End Get
		#tag EndGetter
		#tag Setter
			Set
			  mtramo = value
			  //establece combo de diasemana
			  select case mtramo.dia_semana
			  case 2
			    popdiasemana.ListIndex=0
			  case 3
			    popdiasemana.ListIndex=1
			  case 4
			    popdiasemana.ListIndex=2
			  case 5
			    popdiasemana.ListIndex=3
			  case 6
			    popdiasemana.ListIndex=4
			  case 7
			    popdiasemana.ListIndex=5
			  case 1
			    popdiasemana.ListIndex=6
			  else
			    popdiasemana.ListIndex=0
			  end select
			  
			  //establece combo iniciotramo
			  
			  dim sql as String
			  sql = "select distinct inicioperiodo from horario"
			  dim rs as RecordSet = DATADB.SQLSelect(sql)
			  
			  if rs <> nil and rs.RecordCount>0 then
			    while not rs.EOF
			      Combohorainicio.AddRow(rs.Field("inicioperiodo").StringValue)
			      rs.MoveNext
			    wend
			  end if
			  
			  for e as integer=0 to Combohorainicio.ListCount-1
			    if Combohorainicio.List(e)=mtramo.h_inicio then
			      Combohorainicio.ListIndex=e
			      exit for e
			    end if
			    
			  next
			  
			  //establece como fintramo
			  
			  sql = "select distinct finperiodo from horario"
			  rs = DATADB.SQLSelect(sql)
			  
			  if rs <> nil and rs.RecordCount>0 then
			    while not rs.EOF
			      Combohorafin.AddRow(rs.Field("finperiodo").StringValue)
			      rs.MoveNext
			    wend
			  end if
			  
			  for x as integer=0 to Combohorafin.ListCount-1
			    if Combohorafin.List(x)=mtramo.h_fin then
			      Combohorafin.ListIndex=x
			      exit for x
			    end if
			    
			  next
			  
			  //establece periodo
			  sql = "select distinct periodo from horario"
			  rs = DATADB.SQLSelect(sql)
			  
			  if rs <> nil and rs.RecordCount>0 then
			    while not rs.EOF
			      Comboperiodo.AddRow(rs.Field("periodo").StringValue)
			      rs.MoveNext
			    wend
			  end if
			  
			  for y as integer=0 to Comboperiodo.ListCount-1
			    if Comboperiodo.List(y)=str(mtramo.periodo) then
			      Comboperiodo.ListIndex=y
			      exit for y
			    end if
			    
			  next
			  
			  //establece grupo materia
			  sql = "select id_grupo_materia, denominacion from grupo_materia"
			  rs = DATADB.SQLSelect(sql)
			  
			  if rs <> nil and rs.RecordCount>0 then
			    Popupgrupomateria.AddRow("Seleccione...")
			    dim zz as integer = 1
			    while not rs.EOF
			      Popupgrupomateria.AddRow(rs.Field("denominacion").StringValue)
			      Popupgrupomateria.RowTag(zz)=rs.Field("id_grupo_materia").IntegerValue
			      zz=zz+1
			      rs.MoveNext
			    wend
			  end if
			  
			  for m as integer=0 to Popupgrupomateria.ListCount-1
			    if Popupgrupomateria.RowTag(m)=mtramo.id_grupo_rel then
			      Popupgrupomateria.ListIndex=m
			      exit for m
			    end if
			    
			  next
			  
			  //establece aula
			  sql = "select distinct aula from horario"
			  rs = DATADB.SQLSelect(sql)
			  
			  if rs <> nil and rs.RecordCount>0 then
			    while not rs.EOF
			      Comboaula.AddRow(rs.Field("aula").StringValue)
			      rs.MoveNext
			    wend
			  end if
			  
			  for q as integer=0 to Comboaula.ListCount-1
			    if Comboaula.List(q)=mtramo.aula then
			      Comboaula.ListIndex=q
			      exit for q
			    end if
			    
			  next
			  
			End Set
		#tag EndSetter
		tramo As tramo_horario
	#tag EndComputedProperty


