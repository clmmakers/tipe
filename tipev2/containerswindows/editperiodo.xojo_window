#tag Events PushButton1
	#tag Event
		Sub Action()
		  if me.Caption= "Nuevo" then
		    dim tractual as new tramo_horario
		    mtramo=tractual
		    
		    actualizartramo
		    
		    Comboaula.Text=""
		    popdiasemana.ListIndex=0
		    Combohorafin.Text=""
		    Combohorainicio.Text=""
		    Popupgrupomateria.ListIndex=0
		    Comboperiodo.Text=""
		    me.Caption = "Nuevo"
		  else
		    actualizartramo
		    
		    Comboaula.Text=""
		    popdiasemana.ListIndex=0
		    Combohorafin.Text=""
		    Combohorainicio.Text=""
		    Popupgrupomateria.ListIndex=0
		    Comboperiodo.Text=""
		    me.Caption = "Nuevo"
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton2
	#tag Event
		Sub Action()
		  dim dd as new date
		  dim a,diasem as integer
		  a=dd.DayOfWeek
		  select case a
		  case 2 //lunes
		    diasem=0
		  case 3 //martes
		    diasem=1
		  case 4 //miercoles
		    diasem=2
		  case 5 //jueves
		    diasem=3
		  case 5 //viernes
		    diasem=4
		  case 6 //sabado
		    diasem=5
		  case 7 //domingo
		    diasem=6
		  end select
		  main.listahorariodia(diasem)
		  
		  
		  SELF.Close
		End Sub
	#tag EndEvent
#tag EndEvents
